{
    "configurations": [
        {
            "MIMode": "gdb",
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "name": "Engine",
            "preLaunchTask": "Engine",
            "program": "${workspaceFolder}/bin/engine.dll",
            "request": "launch",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "ignoreFailures": false,
                    "text": "-enable-pretty-printing"
                }
            ],
            "stopAtEntry": false,
            "type": "cppdbg",
            "windows": {
                "miDebuggerPath": "C:/w64devkit/bin/gdb.exe"
            }
        },
        {
            "MIMode": "gdb",
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "name": "Editor",
            "preLaunchTask": "Editor",
            "program": "${workspaceFolder}/bin/editor.exe",
            "request": "launch",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "ignoreFailures": false,
                    "text": "-enable-pretty-printing"
                }
            ],
            "stopAtEntry": false,
            "type": "cppdbg",
            "windows": {
                "miDebuggerPath": "C:/w64devkit/bin/gdb.exe"
            }
        }
    ],
    "version": "0.2.0"
}